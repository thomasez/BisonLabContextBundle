    "description": "This bundle handles contexts as a system for linking internal models/entities to external ones in a flexible configureable way", 
    "keywords": ["symfony context rest"],
    "homepage": "https://github.com/thomasez/BisonLabContextBundle",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Thomas Lundquist",
            "email": "github@bisonlab.no"
        }
    ],
    "require": {
        "symfony/framework-bundle": "^6.3|^7.0"
    },
    "suggest": {
        "guzzlehttp/guzzle": "^7.7"
    },
    "autoload": {
        "psr-4": {
            "BisonLab\\ContextBundle\\": "src/"
        }
    }
}
